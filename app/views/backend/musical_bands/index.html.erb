<p id="notice"><%= notice %></p>

<h1>Listing Musical Bands</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @musical_bands.each do |musical_band| %>
      <tr>
        <td><%= link_to 'Show', backend_musical_band_path(musical_band) %></td>
        <td><%= link_to 'Edit', edit_backend_musical_band_path(musical_band) %></td>
        <td><%= link_to 'Destroy', backend_musical_band_path(musical_band), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Musical band', new_backend_musical_band_path, class: 'btn btn-info' %>
